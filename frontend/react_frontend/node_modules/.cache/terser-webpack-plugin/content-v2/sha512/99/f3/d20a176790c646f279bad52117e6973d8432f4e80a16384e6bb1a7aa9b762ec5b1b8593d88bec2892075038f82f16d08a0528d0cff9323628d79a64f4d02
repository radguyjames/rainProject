etTypeMessage(\\\"Required\\\");\\r\\n      errors++;\\r\\n    }\\r\\n\\r\\n    if (errors === 0) {\\r\\n      props.onPost(keyword);\\r\\n    }\\r\\n  }\\r\\n\\r\\n  // Enables enter key\\r\\n  // useEffect(() => {\\r\\n  //   function handleKeyDown(e) {\\r\\n  //     if (e.keyCode === 13) {\\r\\n  //       handleSave();\\r\\n  //     }\\r\\n  //   }\\r\\n\\r\\n  //   document.addEventListener('keydown', handleKeyDown);\\r\\n\\r\\n  //   return function cleanup() {\\r\\n  //     document.removeEventListener('keydown', handleKeyDown);\\r\\n  //   }\\r\\n  // }, []);\\r\\n\\r\\n  return (\\r\\n    <Container>\\r\\n      <Paper style={{ paddingBottom: '50px', marginBottom: '50px', marginTop: '24px' }}>\\r\\n        <div\\r\\n          style={{\\r\\n            display: \\\"flex\\\",\\r\\n            justifyContent: \\\"space-around\\\",\\r\\n            backgroundColor: '#64B5F6',\\r\\n            marginBottom: '40px',\\r\\n            height: '100px',\\r\\n            borderTopLeftRadius: '4px',\\r\\n            borderTopRightRadius: '4px'\\r\\n          }}>\\r\\n          <h1\\r\\n            style={{ margin: 'auto' }}\\r\\n          >\\r\\n            {props.type === \\\"Edit\\\" ? `Edit Keyword: ${titleOption}` : `Create Keyword`}\\r\\n          </h1>\\r\\n        </div>\\r\\n\\r\\n        <Grid container className={classes.contentGrid}>\\r\\n          <Grid item xs={3} className={classes.gridItem}>\\r\\n            <Typography className={classes.inputLabel}>Keyword:</Typography>\\r\\n          </Grid>\\r\\n          <Grid item xs={9} className={classes.gridItem}>\\r\\n            <TextField\\r\\n              autoFocus\\r\\n              fullWidth\\r\\n              value={keyword.keyword}\\r\\n              onChange={handleKeywordChange}\\r\\n              error={keywordMessage !== \\\"\\\"}\\r\\n              helperText={keywordMessage}\\r\\n            />\\r\\n          </Grid>\\r\\n          <Grid item xs={3} className={classes.gridItem}>\\r\\n            <Typography className={classes.inputLabel}>Type:</Typography>\\r\\n          </Grid>\\r\\n          <Grid item xs={9} className={classes.gridItem}>\\r\\n            <Select\\r\\n              fullWidth\\r\\n              value={keyword.type}\\r\\n              onChange={handleTypeChange}\\r\\n              error={typeMessage !== \\\"\\\"}\\r\\n              helperText={typeMessage}\\r\\n            >\\r\\n              {!types ? null :\\r\\n                types.map(({ id, type }) => (\\r\\n                  <MenuItem key={id} value={id}>{type}</MenuItem>\\r\\n                ))}\\r\\n            </Select>\\r\\n          </Grid>\\r\\n          <Grid item xs={3} className={classes.gridItem}>\\r\\n            <Typography className={classes.inputLabel}>Points:</Typography>\\r\\n          </Grid>\\r\\n          <Grid item xs={9} className={classes.gridItem}>\\r\\n            <TextField\\r\\n              fullWidth\\r\\n              type=\\\"number\\\"\\r\\n              value={keyword.points}\\r\\n              onChange={handlePointsChange}\\r\\n              InputProps={{ inputProps: { min: 0 } }}\\r\\n            >\\r\\n            </TextField>\\r\\n          </Grid>\\r\\n          <Grid item xs={4} style={{ textAlign: \\\"left\\\" }}>\\r\\n            <ButtonGroup size=\\\"small\\\" variant=\\\"contained\\\" style={{ marginTop: '20px' }}>\\r\\n              <Button onClick={handleSave} color='primary'>{props.type === \\\"Edit\\\" ? \\\"Save\\\" : \\\"Create\\\"}</Button>\\r\\n              <Button onClick={handleCancel} color='secondary'>Cancel</Button>\\r\\n            </ButtonGroup>\\r\\n          </Grid>\\r\\n        </Grid>\\r\\n      </Paper>\\r\\n    </Container>\\r\\n  );\\r\\n};\\r\\n\\r\\nexport default App;\\r\\n\",\"import React from \\\"react\\\";\\r\\nimport ManipulateKeyword from './ManipulateKeyword'\\r\\nimport {\\r\\n\\tContainer,\\r\\n} from \\\"@material-ui/core\\\";\\r\\nimport axios from 'axios'\\r\\nimport { useLocation, useHistory } from \\\"reac